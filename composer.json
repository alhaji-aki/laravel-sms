{
    "name": "alhaji-aki/laravel-sms",
    "description": "A package that allows developers integrate sms providers and provide a common api to send their users messages.",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/alhaji-aki/laravel-sms",
    "support": {
        "issues": "https://github.com/alhaji-aki/laravel-sms/issues",
        "source": "https://github.com/alhaji-aki/laravel-sms"
    },
    "keywords": [
        "sms",
        "laravel"
    ],
    "authors": [
        {
            "name": "Abdul Kudus",
            "email": "abdulkudus2922@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/contracts": "^10.0|^11.0",
        "illuminate/notifications": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "propaganistas/laravel-phone": "^5.3"
    },
    "require-dev": {
        "helliomessaging/helliomessaging-laravel-notification-channel": "^1.4",
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.5",
        "thecodingmachine/phpstan-safe-rule": "^1.2",
        "thecodingmachine/safe": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "AlhajiAki\\Sms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlhajiAki\\Sms\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "AlhajiAki\\Sms\\SmsServiceProvider"
            ],
            "aliases": {
                "Sms": "AlhajiAki\\Sms\\Sms"
            }
        }
    },
    "suggest": {
        "helliomessaging/helliomessaging-laravel-notification-channel": "Required to enable support for the Hellio sender (^1.4)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
